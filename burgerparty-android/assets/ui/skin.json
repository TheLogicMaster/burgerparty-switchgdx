{
    com.badlogic.gdx.graphics.g2d.BitmapFont: {
        default-font: {
            file: ui/normal-font.fnt
        }
    },
    com.badlogic.gdx.graphics.Color: {
        green: {
            a: 1, b: 0, g: 1, r: 0
        },
        white: {
            a: 1, b: 1, g: 1, r: 1
        },
        red: {
            a: 1, b: 0, g: 0, r: 1
        },
        black: {
            a: 1, b: 0, g: 0, r: 0
        }
    },
    com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
        default: {
            down: ui/button-down,
            up: ui/button
        },
        toggle: {
            down: ui/button-down,
            checked: ui/button-down,
            up: ui/button
        }
    },
    com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
        default: {
            down: ui/button-down,
            up: ui/button,
            font: default-font,
            fontColor: white
        },
        toggle: {
            down: ui/button-down,
            up: ui/button,
            checked: ui/button-down,
            font: default-font,
            fontColor: white,
            downFontColor: red
        }
    },
    com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
        default: {
            font: default-font,
            fontColor: white
        }
    }
}
